<h1>Sign Up!</h1>
<%= form_for @user do |f| %>
  <div>
    <%= f.label :email %>
    <%= f.text_field :email %>
  </div>
  <div>
    <%= f.label :user_name %>
    <%= f.text_field :user_name %>
  </div>
  <div>
    <%= f.label :industry %>
    <%= f.text_field :industry %>
  </div>
  <div>
    <%= f.label :password %>
    <%= f.password_field :password %>
  </div>
  <div>
    <%= f.label :password_confirmation %>
    <%= f.password_field :password_confirmation %>
  </div>
  <div>
    <%= f.submit 'Create Account!' %>
  </div>
<% end %>
